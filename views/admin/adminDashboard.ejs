<div class="col-lg-12 mb-2">
    <div class="row mt-5">
        <form action="/admin/addDirector" method="post">
            <fieldset>
                <legend>Add Director</legend>
                <div class="form-group">
                    <label for="directorEmail">Email</label>
                    <input type="text" name="directorEmail" class="form-control" id="directorEmail" aria-describedby="directorHelp" placeholder="Enter Email de directeur">
                </div>
                <div class="form-group">
                    <label for="directorPassword">Mot de pass</label>
                    <input type="password" name="directorPassword" class="form-control" id="directorPassword" placeholder="Mot de pass de Directeur">
                </div>
                <div class="form-group">
                    <label for="directorCin">Cin</label>
                    <input type="text" name="directorCin" class="form-control" id="directorCin" placeholder="Cin de Directeur">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </fieldset>
        </form>
    </div>
</div>

<div class="col-lg-12 mb-2">
    <div class="row mt-5">
        <form action="/admin/addCenter" method="post">
            <fieldset>
                <legend>Add Center</legend>
                <div class="form-group">
                    <label for="city">City</label>
                    <input type="text" name="city" class="form-control" id="city" aria-describedby="cityHelp" placeholder="Enter City">
                </div>
                <div class="form-group">
                    <label for="centername">Nom de centre</label>
                    <input type="text" name="centername" class="form-control" id="centername" placeholder="Nom de centre">
                </div>
                <div class="form-group">
                    <label for="directeur">Directeur</label>
                    <input type="text" name="directeur" class="form-control" id="directeur" placeholder="Directeur">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </fieldset>
        </form>
    </div>
</div>

