<div class="row">
    <%- include('./sideBare') %>
  <div class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
        <form action="/admin/AddDoctor" method="post">
            <fieldset>
                <legend>Ajouter Un Medecin</legend>
                <div class="form-group">
                    <label for="email">Email</label>
                    <input type="text" name="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter Email de Medecin">
                </div>
                <div class="form-group">
                    <label for="Cin">Cin</label>
                    <input type="text" name="Cin" class="form-control" id="Cin" placeholder="Cin de Medecin">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </fieldset>
        </form>
  </div>
</div>

